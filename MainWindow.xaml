<Window x:Class="Windows_11_Calendar_Replacement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows_11_Calendar_Replacement"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="CurrentYearLabel" Content="Label" HorizontalAlignment="Center" VerticalAlignment="Top" Height="24" Width="316" HorizontalContentAlignment="Center"/>
        <Grid x:Name="DatesGrid" Margin="0,39,0,0"/>
        <Label x:Name="CurrentMonthLabel" Content="Label" HorizontalAlignment="Center" Margin="0,13,0,0" VerticalAlignment="Top" Width="157" HorizontalContentAlignment="Center" />
        <Button x:Name="NextMonth" Content="Next" HorizontalAlignment="Left" Margin="657,4,0,0" VerticalAlignment="Top" Height="20" Width="133" Click="NextMonth_Click"/>
        <Button x:Name="PrevMonth" Content="Prev" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Height="20" Width="133" Click="PrevMonth_Click"/>

    </Grid>


</Window>
